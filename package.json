{
  "name": "fractions-equality",
  "version": "1.2.0-dev.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/fractions-equality.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "FRACTIONS_EQUALITY",
    "phetLibs": [
      "fraction-matcher",
      "fractions-common",
      "twixt",
      "vegas"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simFeatures": {
      "supportsSound": true
    },
    "simulation": true,
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "FRACTIONS_EQUALITY/screen.equalityLab",
      "FRACTIONS_EQUALITY/screen.game"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "parserOptions": {
          "project": [
            "../chipper/tsconfig/eslint-all/tsconfig.json"
          ]
        }
      }
    ]
  }
}